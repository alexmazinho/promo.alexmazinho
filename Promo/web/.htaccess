<IfModule mod_rewrite.c>
    RewriteEngine On

    #<IfModule mod_vhost_alias.c>
    #    RewriteBase /
    #</IfModule>

	RewriteCond %{HTTP_HOST} ^promo.dev
	RewriteRule (.*) http://www.promo.dev/$1 [R=301,L]

    RewriteCond %{REQUEST_FILENAME} !-f
    #RewriteRule ^(.*)$ app.php [QSA,L]
